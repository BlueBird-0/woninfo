<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="match_parent"
        android:layout_height="122dp"
        android:background="@drawable/all_shape_graybox"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/setting_imageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginStart="24dp"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@mipmap/ic_appicon_round" />

        <TextView
            android:id="@+id/setting_nikname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="24dp"
            android:text="닉네임"
            android:textColor="@color/Theme_Blue"
            android:textSize="@dimen/list_item_bigtitle_size"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/setting_imageView"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/setting_year"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:text="3학년"
            android:textColor="@color/Theme_Gray"
            android:textSize="@dimen/bigtitle_size"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/setting_imageView"
            app:layout_constraintTop_toBottomOf="@+id/setting_nikname" />

        <TextView
            android:id="@+id/setting_department"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:text="컴퓨터공학과"
            android:textColor="@color/Theme_Gray"
            android:textSize="@dimen/bigtitle_size"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/setting_year"
            app:layout_constraintTop_toBottomOf="@+id/setting_nikname" />

        <ImageView
            android:id="@+id/setting_email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/ic_message"
            app:layout_constraintStart_toEndOf="@+id/setting_imageView"
            app:layout_constraintTop_toBottomOf="@+id/setting_year" />

        <TextView
            android:id="@+id/setting_email_textview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="qqqqqq@naver.com"
            android:textColor="@color/Theme_Gray"
            android:textSize="@dimen/bigtitle_size"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/setting_email"
            app:layout_constraintTop_toBottomOf="@+id/setting_department" />

    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
        android:id="@+id/setting_btn1"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/all_ripple_click_roundrectanglebox"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2">

        <ImageView
            android:id="@+id/setting_next"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            android:src="@drawable/ic_listbtn_image"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/setting_listname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginStart="24dp"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            android:text="알림"
            android:textColor="@color/Theme_Blue"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
