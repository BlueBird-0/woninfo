<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraintLayout5"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/Theme_Beige"
        android:orientation="vertical">

        <android.support.constraint.ConstraintLayout
            android:id="@+id/freebtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="7dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="24dp"
            android:background="@drawable/boxbutton"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/a_25_text01"
                android:layout_width="175dp"
                android:layout_height="99dp"
                android:layout_margin="15dp"
                android:layout_marginStart="8dp"
                android:text="자유게시판"
                android:textColor="@color/Theme_Blue"
                android:textSize="@dimen/menubigtitle_size"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="15dp" />


        </android.support.constraint.ConstraintLayout>

        <android.support.constraint.ConstraintLayout
            android:id="@+id/changebtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="7dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/boxbutton"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/freebtn">

            <TextView
                android:id="@+id/a_25_text02"
                android:layout_width="175dp"
                android:layout_height="99dp"
                android:layout_margin="15dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="교환장터"
                android:textColor="@color/Theme_Blue"
                android:textSize="@dimen/menubigtitle_size"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


        </android.support.constraint.ConstraintLayout>

        <android.support.constraint.ConstraintLayout
            android:id="@+id/foodbtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="7dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/boxbutton"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/changebtn">

            <TextView
                android:id="@+id/textVie3"
                android:layout_width="240dp"
                android:layout_height="100dp"
                android:layout_margin="15dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="맛집게시판"
                android:textColor="@color/Theme_Blue"
                android:textSize="@dimen/menubigtitle_size"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


        </android.support.constraint.ConstraintLayout>

        <android.support.constraint.ConstraintLayout
            android:id="@+id/secretbtn"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="7dp"
            android:layout_marginBottom="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="32dp"
            android:background="@drawable/boxbutton"
            app:layout_constraintBottom_toTopOf="@+id/foodbtn"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/freebtn"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/a_25_text11"
                android:layout_width="229dp"
                android:layout_height="99dp"
                android:layout_margin="15dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="비밀게시판"
                android:textColor="@color/Theme_Blue"
                android:textSize="@dimen/menubigtitle_size"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


        </android.support.constraint.ConstraintLayout>


        <android.support.constraint.ConstraintLayout


                android:id="@+id/button"
                android:layout_width="107dp"
                android:layout_height="206dp"
                android:layout_margin="7dp"
                android:layout_marginBottom="16dp"
                android:layout_marginEnd="16dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/boxbutton"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/foodbtn"
                app:layout_constraintTop_toBottomOf="@+id/secretbtn"
                app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="229dp"
            android:layout_height="99dp"
            android:layout_margin="15dp"
            android:layout_marginStart="12dp"
            android:text="홍보\n게시판"
            android:textColor="@color/Theme_Blue"
            android:textSize="@dimen/menubigtitle_size"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/button"
            tools:layout_editor_absoluteY="319dp" />


        </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
        android:id="@+id/busbtn"
        android:layout_width="255dp"
        android:layout_height="133dp"
        android:layout_margin="7dp"
        android:layout_marginStart="36dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/boxbutton"
        app:layout_constraintBottom_toBottomOf="@+id/constraintLayout5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <TextView
            android:id="@+id/textVie6"
            android:layout_width="233dp"
            android:layout_height="100dp"
            android:layout_margin="15dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:text="버스노선도"
            android:textColor="@color/Theme_Blue"
            android:textSize="@dimen/menubigtitle_size"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


    </android.support.constraint.ConstraintLayout>
</android.support.constraint.ConstraintLayout>